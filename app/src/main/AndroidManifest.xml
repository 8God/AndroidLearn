<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="seu.com.androidlearn">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/my_app_name"
        tools:replace="android:label"
        android:supportsRtl="true">
        <receiver android:name=".broadcast.MyBroadcast">
            <intent-filter>
                <action android:name="com.seu.cc.test"/>
            </intent-filter>
        </receiver>
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>


            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="wxy"/>
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity"/>
        <!--完全无效-->
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
        <activity
            android:name=".statusbar.StatusBarTestActivity"
            android:theme="@style/StatusBarTran"/>
        <activity android:name=".touch.TouchTestActivity"/>
        <activity android:name=".customview.MultiPicActivity"/>
        <activity android:name=".dialog.AlertDialogActivity"/>
        <activity android:name=".battery.BatteryActivity"/>
        <activity android:name=".swipe.RecyclerActivity"/>
        <activity android:name=".recycle.RecyclerViewActivity"/>
        <activity android:name=".leak.LeakActivity"/>
        <activity android:name=".js.WebviewActivity"/>
        <activity android:name=".file.FileActivity"/>
        <activity android:name=".okhttpcache.OkHttpCacheActivity"/>
        <activity android:name=".test.TestActivity">
        </activity>
        <activity android:name=".dynamicviewpager.ViewPagerActivity"/>
        <activity android:name=".broadcast.BroadcastActivity"/>
        <activity android:name=".ioc.runtime.RuntimeActivity"/>
        <activity android:name=".hook.HookActivity"/>
    </application>

</manifest>